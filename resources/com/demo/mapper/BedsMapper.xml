<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.BedsMapper">

    <resultMap id="BaseResultMap" type="com.demo.pojo.Beds">
            <id property="bedId" column="bed_id" />
            <result property="roomId" column="room_id" />
            <result property="bedNumber" column="bed_number" />
            <result property="status" column="status" />
            <result property="residentName" column="resident_name" />
            <result property="residentPhone" column="resident_phone" />
            <result property="checkInDate" column="check_in_date" />
            <result property="checkOutDate" column="check_out_date" />
            <result property="notes" column="notes" />
    </resultMap>

    <sql id="Base_Column_List">
        bed_id,room_id,bed_number,status,resident_name,resident_phone,
        check_in_date,check_out_date,notes
    </sql>
</mapper>
